{
    "name": "mskocik/forgery",
    "description": "Nette automatic DI container",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/mskocik/auryn"
        }
    ],
    "require": {
        "php": ">=8.1",
        "nette/di": "^3.0",
		"nette/utils": "^3.0",
        "martin-hughes/auryn": "dev-nullfix"
    },
    "require-dev": {
        "nette/bootstrap": "^3.0",
        "nette/neon": "^3.0",
        "nette/tester": "^2.3",
        "nette/caching": "^2.5"
    },
    "autoload": {
        "psr-4": {
            "Mskocik\\ForgeryDI\\": "src"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
