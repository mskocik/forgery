{
    "name": "mskocik/forgery",
    "description": "Nette automatic DI container",
    "require": {
        "php": ">=8.1",
        "nette/di": "~3.1",
        "nette/utils": "~3.0 || ^4.0"
    },
    "require-dev": {
        "nette/bootstrap": "^3.2",
        "nette/neon": "~3.0",
        "nette/tester": "~2.3",
        "nette/caching": "^3.3"
    },
    "autoload": {
        "psr-4": {
            "Mskocik\\ForgeryDI\\": "src",
            "Auryn\\": "lib"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/tester tests",
        "watch": "vendor/bin/tester tests --watch=src --watch=tests/cases"
    },
    "minimum-stability": "dev",
    "prefer-stable": true}
