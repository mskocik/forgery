{
    "name": "mskocik/forgery",
    "description": "Nette automatic DI container",
    "require": {
        "php": ">=8.1",
        "nette/di": "^3.0",
		"nette/utils": "^3.0"
    },
    "require-dev": {
        "nette/bootstrap": "^3.0",
        "nette/neon": "^3.0",
        "nette/tester": "^2.3",
        "nette/caching": "^2.5"
    },
    "autoload": {
        "psr-4": {
            "Mskocik\\ForgeryDI\\": "src",
            "Auryn\\": "lib"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
